                <li><a href="https://wrapbootstrap.com/?ref=bsw" target="_blank">WrapBootstrap</a></li>
            </ul>

        </div>
    </div>
</div>
<div class="container">
    <div class="col-md-4">
        <div class="dashboard-stat red-intense" align="center" style="border-width: 5px; border-style: solid">
            <div class="visual">
                <i class="fa fa-users"></i>

            </div>
            <div class="details">
                <div class="number">
                    <%= online %>
                </div>
                <div class="desc">
                    Players Online (in past 2 hours)
                </div>
            </div>

        </div>
    </div>
    <div class="col-md-4">
        <div class="dashboard-stat red-intense" align="center" style="border-width: 5px; border-style: solid">
            <div class="visual">
                <i class="fa fa-cloud-upload"></i>
            </div>
            <div class="details">

                <div class="number">
                    <%= server1.raw.numplayers %> / <%= server1.maxplayers %>
                </div>
                <div class="desc">

                    <a href="#">1v1 MULTI-ARENAS #1</a>

                </div>

            </div>

        </div>
    </div>
    <div class="col-md-4">
        <div class="dashboard-stat red-intense" align="center" style="border-width: 5px; border-style: solid">
            <div class="visual">
                <i class="fa fa-cloud-upload"></i>

            </div>
            <div class="details">
                <div class="number">
                    <%= server2.raw.numplayers %> / <%= server2.maxplayers %>
                </div>
                <div class="desc">
                    <a href="#">Competitive Retakes #1</a>
                </div>
            </div>

        </div>
    </div>
    <div class="col-md-6">
        <div class="panel panel-danger">
            <div class="panel-heading" align="center"><h4>Top 10 Players by Rank (Active in 30 days)</h4></div>
            <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Wins</th>
                    <th>Losses</th>
                    <th>Rating</th>
                    <th>Last Date</th>
                </tr>
                </thead>
                <% for(var i = 0; i < data.length; i++) { %>
                <tr>
                    <!--<a href="https://steamcommunity.com/profiles/<%= data[i].auth %>/"><%= data[i].name %></a>-->
                    <th id="link<%= i %>"><%= data[i].name %></th>
                    <th><%= data[i].wins %></th>
                    <th><%= data[i].losses %></th>
                    <th><%= data[i].rating %></th>
                    <th><%= new Date(data[i].lastTime * 1000) %></th>
                </tr>
                <% } %>
            </table>
        </div>
    </div>
    <div class="col-md-6" id="profile">
        <div class="panel panel-danger" id="clicked">
            <div class="panel-heading" align="center"><h4>Your Profile</h4></div>
            <div class="media">
                <div class="media-left" style="padding-left: 5px">
                    <a href="https://steamcommunity.com/profiles/<%= player[0].auth %>/">

                    </a>
                </div>

                <div class="media-body">
                    <h4 class="media-heading"><a
                                href="https://steamcommunity.com/profiles/<%= player[0].auth %>/"><%= player[0].name %></a>
                    </h4>

                    <p>Wins: <%= player[0].wins %></p>

                    <p>Loses: <%= player[0].losses %></p>

                    <p>Rating: <%= player[0].rating %></p>

                    <p>Last Played: <%= new Date(player[0].lastTime * 1000) %></p>
                </div>
            </div>

        </div>
    </div>
</div>
